{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "A clean start with babel, eslint (airbnb) and webpack for browser environment.",
  "main": "bundle.js",
  "scripts": {
    "start": "npm run build && npm run server",
    "server": "webpack-dev-server --config webpack.config.js --open",
    "build": "node_modules/.bin/webpack",
    "watch": "node_modules/.bin/webpack --watch",
    "styles": "node_modules/.bin/sass sass/src/index.scss sass/dist/styles.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neulandagentur/webpack-starter.git"
  },
  "keywords": [
    "webpack",
    "babel",
    "eslint",
    "boilerplate"
  ],
  "author": "NEULAND. Medienagentur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neulandagentur/webpack-starter/issues"
  },
  "homepage": "https://github.com/neulandagentur/webpack-starter#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "sass": "^1.17.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
